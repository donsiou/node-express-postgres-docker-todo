{
  "name": "node-express-postgres-docker-todo",
  "version": "1.0.0",
  "description": "Simple node express postgres docker ToDo app for starting new services and testing docker deployments with kubernetes.",
  "scripts": {
    "lint": "eslint .",
    "start": "node server",
    "start:dev": "nodemon server.js",
    "test": "docker-compose up -d && docker-compose exec -T todo-app npm run test:internal",
    "test:internal": "eslint . && nyc --reporter=html --reporter=text-summary mocha --recursive",
    "test:travis": "NODE_ENV=travis eslint . && nyc mocha --recursive --timeout 30000 && nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jtwebman/node-express-postgres-docker-todo.git"
  },
  "main": "server.js",
  "author": "JT Turner",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jtwebman/node-express-postgres-docker-todo/issues"
  },
  "homepage": "https://github.com/jtwebman/node-express-postgres-docker-todo#readme",
  "dependencies": {
    "async-retry": "^1.3.1",
    "config": "^3.3.3",
    "express": "^4.17.1",
    "lodash": "^4.17.20",
    "pg-promise": "^10.8.1",
    "stoppable": "^1.1.0",
    "winston": "^3.3.3"
  },
  "engines": {
    "node": ">= 14"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "coveralls": "^3.1.0",
    "eslint": "^7.15.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-prettier": "^3.3.0",
    "mocha": "^8.2.1",
    "nodemon": "^2.0.6",
    "nyc": "^15.1.0",
    "prettier": "2.2.1",
    "supertest": "^6.0.1"
  }
}
